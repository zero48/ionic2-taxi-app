{
  "name": "ionic2-taxi-app",
  "version": "0.1.0",
  "description": "Ionic 2 Taxi App - A simple starting point for building 'on-demand/rideshare taxi' ionic application ( something like Uber, Lyft or Sidecar .. )",
  "keywords": [
    "ionic",
    "ionic2",
    "angular2",
    "google-maps",
    "webpack",
    "typescript",
    "yarn"
  ],
  "author": {
    "name": "Damien Dell'Amico",
    "email": "damien.dellamico@gmail.com",
    "url": "https://github.com/ddellamico"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddellamico/ionic2-taxi-app.git"
  },
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "clean": "bnr clean",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "outdated": "yarn outdated",
    "precommit": "npm run lint",
    "prepush": "npm test"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "clean": {
      "command": "ionic-app-scripts clean"
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic build ios --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic build android --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/compiler-cli": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/platform-server": "2.4.8",
    "@angular/tsc-wrapped": "~0.5.2",
    "@ionic/storage": "2.0.0",
    "ionic-angular": "2.2.0",
    "ionic-native": "2.4.1",
    "ionicons": "3.0.0",
    "lodash-es": "^4.17.4",
    "node-uuid": "^1.4.7",
    "rxjs": "5.0.1",
    "sw-toolbox": "3.4.0",
    "uuid": "^2.0.2",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.1.4",
    "@types/googlemaps": "^3.26.2",
    "@types/jasmine": "2.5.41",
    "@types/lodash": "ts2.0",
    "@types/node-uuid": "0.0.28",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.8",
    "better-npm-run": "0.0.14",
    "codelyzer": "2.0.1",
    "commitizen": "^2.9.5",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^2.0.0",
    "finalhandler": "^1.0.0",
    "husky": "^0.13.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "0.5.1",
    "serve-static": "^1.11.2",
    "source-map-loader": "^0.1.6",
    "standard-version": "^4.0.0",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "^2.1.0",
    "tslint": "4.0.0",
    "tslint-loader": "^3.4.2",
    "typedoc": "^0.5.7",
    "typescript": "~2.0.10",
    "validate-commit-msg": "^2.11.1",
    "webpack": "2.2.1",
    "xml2js": "^0.4.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_webpack": "./config/webpack.config.js"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  }
}
